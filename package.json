{
  "name": "sample-test-runner",
  "version": "1.0.0",
  "description": "",
  "main": "js/main.js",
  "scripts": {
    "start": "browserify -t [ stringify --extensions [.html] ] js/main.js -o build/bundle.js && http-server",
    "dev-server": "http-server",
    "watch": "watchify -t [ stringify --extensions [.html] ] js/main.js -o build/bundle.js -v ",
